{
  "expo": {
    "name": "FinGuard",
    "slug": "finguard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nexify.finguard",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This app uses notifications to remind you about your financial goals and budget alerts.",
        "NSCameraUsageDescription": "This app uses the camera to scan receipts for expense tracking.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to attach receipt images to transactions."
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nexify.finguard",
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ],
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#4F8EF7",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "da4ba51f-c610-48fc-9a60-7bd34be44864"
      }
    },
    "owner": "satyaranjan2005",
    "updates": {
      "url": "https://u.expo.dev/da4ba51f-c610-48fc-9a60-7bd34be44864"
    }
  }
}
